{
  "name": "newton.js",
  "version": "0.0.0",
  "description": "Javascript lightweight 2D rigid body physics engine",
  "main": "dist/newton.cjs.js",
  "browser": "dist/newton.esm.js",
  "type": "module",
  "scripts": {
    "start": "run-p dev:*",
    "dev:build_esm": "./node_modules/.bin/esbuild src/index.js --format=esm --bundle --target=chrome60,firefox60,safari12 --outfile=dist/newton.esm.js --watch",
    "dev:demo": "./node_modules/.bin/browser-sync start -s -w --directory --startPath ./index.html",
    "build": "run-p build:*",
    "build:iife": "./node_modules/.bin/esbuild src/iife.js --format=iife --bundle --target=chrome60,firefox60,safari12 --outfile=dist/newton.js",
    "build:iife-min": "./node_modules/.bin/esbuild src/iife.js --format=iife --bundle --minify --target=chrome60,firefox60,safari12 --outfile=dist/newton.js",
    "build:cjs": "./node_modules/.bin/esbuild src/index.js --format=cjs  --bundle --target=chrome60,firefox60,safari12 --outfile=dist/newton.cjs.js",
    "build:cjs-min": "./node_modules/.bin/esbuild src/index.js --format=cjs  --bundle --minify --target=chrome60,firefox60,safari12 --outfile=dist/newton.cjs.js",
    "build:esm": "./node_modules/.bin/esbuild src/index.js --format=esm  --bundle --target=chrome60,firefox60,safari12 --outfile=dist/newton.esm.js",
    "build:esm-min": "./node_modules/.bin/esbuild src/index.js --format=esm  --bundle --minify --target=chrome60,firefox60,safari12 --outfile=dist/newton.esm.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "physics",
    "rigid",
    "collision",
    "constraints",
    "lightweight",
    "tiny",
    "matter-js",
    "box2d"
],
  "author": "Dmitrii Vasilev",
  "homepage": "newton.js.org",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "esbuild": "^0.20.1",
    "npm-run-all": "^4.1.5"
  }
}
